<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<Pane prefHeight="125.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.nc.controller.NotificationController">
    <Label layoutX="14.0" layoutY="6.0" text="Актуальные задачи" />
    <Label fx:id="titleLabel" layoutX="14.0" layoutY="40.0" text="Label" GridPane.columnIndex="1" />
    <Button layoutX="30.0" layoutY="80.0" mnemonicParsing="false" onAction="#okButton" text="OK">
        <padding>
            <Insets bottom="5.0" left="30.0" right="30.0" top="5.0" />
        </padding>
    </Button>
    <Button layoutX="170.0" layoutY="80.0" mnemonicParsing="false" onAction="#delayButton" text="Отложить">
        <padding>
            <Insets bottom="5.0" left="30.0" right="30.0" top="5.0" />
        </padding>
    </Button>
</Pane>